<xr-demo-viewer
  title="AR 2DMarker案例"
  intro="Marker识别案例，点击可以上传2DMarker。识别成功后，会于屏幕绘制一个纯色立方体展示位置。"
  code="{{xmlCode}}"
>
  <xr-ar-2dmarker
    disable-scroll
    id="main-frame"
    width="{{renderWidth}}"
    height="{{renderHeight}}"
    style="width:{{width}}px;height:{{height}}px;top:{{top}}px;left:{{left}}px;display:block;"
    markerImg="{{markerImg}}"
  />

  <button bindtap="handleChangeMarkerImg">
    点击替换Marker图像
  </button>
</xr-demo-viewer>