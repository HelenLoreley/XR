<xr-demo-viewer
  title="卡其脱离太"
  intro="xr-frame团队的办公区和成员三维重建的例子。点击`卡其脱离太`开始奔跑，奔跑之后点击成员可以跳跃。"
  code="{{xmlCode}}"
>
  <xr-scan-team
    disable-scroll
    id="main-frame"
    width="{{renderWidth}}"
    height="{{renderHeight}}"
    style="width:{{width}}px;height:{{height}}px;top:{{top}}px;left:{{left}}px;display:block;"
    loaded="{{loaded}}"
    run="{{run}}"
    bind:assetsLoaded="handleLoaded"
    bind:syncPositions="handleSyncPositions"
  />

  <view wx:if="{{!loaded}}">等待加载完成...</view>
  <button wx:if="{{loaded}}" bindtap="handleRun">卡其脱离太！</button>
</xr-demo-viewer>