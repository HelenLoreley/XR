<xr-demo-viewer
  title="三维透视案例"
  intro="基于Stencil实现的传送门特效案例. Model By @Omar Faruq Tawsif"
>
  <xr-classic-portal
    disable-scroll
    id="main-frame"
    width="{{renderWidth}}"
    height="{{renderHeight}}"
    style="width:{{width}}px;height:{{height}}px;top:{{top}}px;left:{{left}}px;display:block;"
    markerImg="{{markerImg}}"
  />
</xr-demo-viewer>