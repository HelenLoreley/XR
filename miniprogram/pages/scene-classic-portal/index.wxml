<xr-demo-viewer
  title="传送门效果案例"
  intro="基于 RenderTexture 实现的基本传送门效果案例."
>
  <xr-classic-portal
    disable-scroll
    id="main-frame"
    width="{{renderWidth}}"
    height="{{renderHeight}}"
    style="width:{{width}}px;height:{{height}}px;top:{{top}}px;left:{{left}}px;display:block;"
  />
</xr-demo-viewer>