<xr-demo-viewer
  title="平面视频案例"
  intro="基于 2DMarker 实现，扫描工卡照片显示对应的生活场景."
>
  <xr-classic-video
    disable-scroll
    id="main-frame"
    width="{{renderWidth}}"
    height="{{renderHeight}}"
    style="width:{{width}}px;height:{{height}}px;top:{{top}}px;left:{{left}}px;display:block;"
    markerImg="{{markerImg}}"
  />
</xr-demo-viewer>